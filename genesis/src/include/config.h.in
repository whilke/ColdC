#ifndef cdc_config_h
#define cdc_config_h

#define VERSION_MAJOR 1
#define VERSION_MINOR 1
#define VERSION_PATCH 13
#define VERSION_RELEASE "ALPHA"
#cmakedefine SYSTEM_TYPE ${SYSTEM_TYPE}
/* Define to empty if the keyword does not work.  */
#cmakedefine const

/* Define if you don't have vprintf but do have _doprnt.  */
#cmakedefine HAVE_DOPRNT

/* Define if you have the strftime function. C */
#cmakedefine HAVE_STRFTIME

/* Define if you have <sys/wait.h> that is POSIX.1 compatible.  */
#cmakedefine HAVE_SYS_WAIT_H

/* Define for the right header for dbm routines */
#define DBM_H_FILE <ndbm.h>
#define DB_DBM_HSEARCH 1

/* Define if your struct dirent has d_namlen */
#cmakedefine HAVE_D_NAMLEN

/* Define if your struct tm has tm_zone.  */
#define HAVE_TM_ZONE

/* Define if your struct tm has tm_gmtoff */
#cmakedefine HAVE_TM_GMTOFF

/* Define if you don't have tm_zone but do have the external array
   tzname.  */
#cmakedefine HAVE_TZNAME

/* Define if you have <vfork.h>. C */
#cmakedefine HAVE_VFORK_H

/* Define if you have the vprintf function.  C*/
#cmakedefine HAVE_VPRINTF

/* Define if you inet_aton() C*/
#cmakedefine HAVE_INET_ATON

/* Define if you have rint() C*/
#cmakedefine HAVE_RINT

/* Define as __inline if that's what the C compiler calls it.  */
#cmakedefine inline

/* Define to `long' if <sys/types.h> doesn't define.  */
#cmakedefine off_t

/* Define to `int' if <sys/types.h> doesn't define.  */
#cmakedefine pid_t

/* Define as the return type of signal handlers (int or void).  */
#cmakedefine RETSIGTYPE

/* Define if the `S_IS*' macros in <sys/stat.h> do not work properly.  */
#cmakedefine STAT_MACROS_BROKEN

/* Define if you have the ANSI C header files.  */
#cmakedefine STDC_HEADERS

/* Define if `sys_siglist' is declared by <signal.h>.  */
#cmakedefine SYS_SIGLIST_DECLARED

/* Define if you can safely include both <sys/time.h> and <time.h>.  */
#cmakedefine TIME_WITH_SYS_TIME

/* Define if your <sys/time.h> declares struct tm.  */
#cmakedefine TM_IN_SYS_TIME

/* Define vfork as fork if vfork does not work.  */
#cmakedefine vfork

/* Sizes */
#cmakedefine SIZEOF_CHAR @SIZEOF_CHAR@
#cmakedefine SIZEOF_SHORT @SIZEOF_SHORT@
#cmakedefine SIZEOF_INT @SIZEOF_INT@
#cmakedefine SIZEOF_LONG @SIZEOF_LONG@
#cmakedefine SIZEOF_LLONG @SIZEOF_LLONG@
#cmakedefine SIZEOF_FLOAT @SIZEOF_FLOAT@
#cmakedefine SIZEOF_DOUBLE @SIZEOF_DOUBLE@
#cmakedefine SIZEOF_LDOUBLE @SIZEOF_LDOUBLE@

/* Define if you have the getrusage function.  C*/
#cmakedefine HAVE_GETRUSAGE

/* Define if you have the gettimeofday function.  */
#cmakedefine HAVE_GETTIMEOFDAY

/* Define if you have the memmove function.  */
#cmakedefine HAVE_MEMMOVE

/* Define if you have the mkdir function.  */
#cmakedefine HAVE_MKDIR

/* Define if you have the select function.  */
#cmakedefine HAVE_SELECT

/* Define if you have the socket function.  */
#cmakedefine HAVE_SOCKET

/* Define if you have the strchr function.  */
#cmakedefine HAVE_STRCHR

/* Define if you have the strcspn function.  */
#cmakedefine HAVE_STRCSPN

/* Define if you have the strerror function.  */
#cmakedefine HAVE_STRERROR

/* Define if you have the strstr function.  */
#cmakedefine HAVE_STRSTR

/* Define if you have the finite function.  */
#cmakedefine HAVE_FINITE

/* Define if you have the <fcntl.h> header file.  */
#cmakedefine HAVE_FCNTL_H

/* Define if you have the <sys/file.h> header file.  */
#cmakedefine HAVE_SYS_FILE_H

/* Define if you have the <sys/time.h> header file.  */
#cmakedefine HAVE_SYS_TIME_H

/* Define if you have the <unistd.h> header file.  */
#cmakedefine HAVE_UNISTD_H

#cmakedefine RESTRICTIVE_FILES

#define CACHE_WIDTH 61
#define CACHE_DEPTH 10

